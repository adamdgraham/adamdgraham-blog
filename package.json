{
  "name": "adamdgraham-blog",
  "version": "1.0.0",
  "description": "Blog at adamdgraham.com",
  "main": "app-server.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "development": "cp views/index.html public/index.html && NODE_ENV=development webpack &    & webpack-dev-server --content-base public/ --hot --inline --devtool inline-source-map --hi story-api-fallback",
    "production": "rm -rf public/index.html && NODE_ENV=production webpack -p && NODE_ENV=p    roduction node app.js",
    "start": "npm run production",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
