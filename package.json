  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "react-hot-loader": "1.3.0",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "lodash": "^4.13.1",
    "react": "15.2.1",
    "react-dom": "15.2.1",
    "react-router": "2.6.0",
    "redux": "^3.5.2",
    "webpack": "1.13.1"
  },
  "engines": {
    "node": "6.3.0"
  }
